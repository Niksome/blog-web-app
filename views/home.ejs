<%- include("partials/header"); %>
<h1>Home</h1>

<p><%= startingContent %></p>

<% posts.forEach(function(post) { %>
<div class="post">
  <h2><%= post.title %></h2>
  <p>
    <%= post.content.substring(0, 100) + " ..." %>
    <a href="/posts/<%=post.title%>">Read more</a>
  </p>
</div>
<% }); %> <%- include("partials/footer"); %>
